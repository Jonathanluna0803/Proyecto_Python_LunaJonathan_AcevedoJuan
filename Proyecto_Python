booleanito = True
while (booleanito == True):
    print("Bienvenido a campuslands!!")
    print("Cual es tu rol")
    print("1. Camper")
    print("2. Trainer")
    print("3. Coordinador")
    print("4. Salir")
    rolOpcion = int(input(": "))
    if (rolOpcion == 1):  
        idCamper = input("\nIngresa tu número de identificación: ")
        for camper in Campers:
            if camper["#Identificacion"] == idCamper:
                booleanito2 = True
                while (booleanito2 == True):
                    print(f"Bienvenido, {camper['Nombre']} {camper['Apellido']}!")
                    print(f"MENÚ CAMPER - {camper['Nombre']} {camper['Apellido']}")
                    print("1. Consultar mi información personal")
                    print("2. Ver mi estado académico")
                    print("3. Ver mis notas por módulo")
                    print("4. Ver mi ruta y trainer asignado")
                    print("5. Cerrar sesión")
                    opcionCamper = int(input("Selecciona una opción: "))
                    if (opcionCamper == 1):
                        print(f"INFORMACIÓN PERSONAL:")
                        print(f"   Nombre: {camper['Nombre']} {camper['Apellido']}")
                        print(f"   ID: {camper['#Identificacion']}")
                        print(f"   Dirección: {camper['Direccion']}")
                        print(f"   Acudiente: {camper['Acudiente']}")        
                    elif (opcionCamper == 2):
                        print(f"ESTADO ACADÉMICO:")
                        print(f"   Estado: {camper['Estado']}")
                        print(f"   Riesgo: {camper['Riesgo']}")
                        print(f"   Grupo: {camper['Grupo']}")
                    elif (opcionCamper == 3):
                        print("MIS NOTAS:")
                        print(f"   Prueba inicial teórica: {camper['NotaPruebaTeorica']}")
                        print(f"   Prueba inicial práctica: {camper['NotaPruebaPractica']}")
                        print(f"   Promedio general: {camper['NotaPromedioGeneral']}")
                        print(f"   Quizzes: {camper['NotaQuizzes']}")
                    elif (opcionCamper == 4):
                        print(f"RUTA ASIGNADA: {camper['Grupo']}")
                        if camper["Grupo"] == "P1":
                            print("TRAINER ASIGNADO: Pedro")
                        elif camper["Grupo"] == "P2":
                            print("TRAINER ASIGNADO: Pedro")
                        elif camper["Grupo"] == "P3":
                            print("TRAINER ASIGNADO: Pedro")
                        elif camper["Grupo"] == "J1":
                            print("TRAINER ASIGNADO: Jolver")
                        elif camper["Grupo"] == "P2":
                            print("TRAINER ASIGNADO: Pedro")
                        elif camper["Grupo"] == "Cr1":
                            print("TRAINER ASIGNADO: Cristian")
                        elif camper["Grupo"] == "Cr2":
                            print("TRAINER ASIGNADO: Cristian")
                        elif camper["Grupo"] == "E1":
                            print("TRAINER ASIGNADO: Edwin")
                        elif camper["Grupo"] == "K1":
                            print("TRAINER ASIGNADO: Kevin")
                        elif camper["Grupo"] == "Ca1":
                            print("TRAINER ASIGNADO: Carlos")
                        elif camper["Grupo"] == "Ca2":
                            print("TRAINER ASIGNADO: Carlos")
                    elif (opcionCamper == 5):
                        print("\n Sesión cerrada. ¡Hasta pronto!")
                        booleanito2 = False
    elif (rolOpcion == 2):  
        idTrainer = input("\n Ingresa tu número de identificación: ")
        for trainer in Trainers:
            if trainer["#Identificacion"] == idTrainer:
                booleanito3 = True
                while (booleanito3 == True):
                    print(f"\n Bienvenido/a, {trainer['Nombre']}!")
                    print(f"MENÚ TRAINER - {trainer['Nombre']}")
                    print("1. Registrar notas de módulo")
                    print("2. Ver campers asignados")
                    print("3. Ver mi horario y salón")
                    print("4. Generar reporte de bajo rendimiento")
                    print("5. Cerrar sesión")
                    opcionTrainer = int(input(": "))
                    if (opcionTrainer == 1):
                        print("REGISTRO DE NOTAS POR MÓDULO")
                        idCamper = input("ID del camper: ")                    
                        for camper in Campers:
                            if camper["#Identificacion"] == idCamper and camper["Trainer"] == trainer:
                                print(f"Camper {camper['Nombre']}")
                                print(f"Trainer {trainer['Nombre']} que nota desea registrar")
                                print("1. Nota quizzes")
                                print("2. Nota prueba practica")
                                registroNotas = int(input(": "))
                                if (registroNotas == 1):
                                    notaQuizz = float(input("Que nota desea ingresar: "))
                                    camper["NotaQuizzes"] = notaQuizz
                                    print("!Nota Registrada¡")
                                elif (registroNotas == 2):
                                    notaPractica = float(input("Que nota desea ingresar:"))
                                    camper["NotaPruebaPractica"] = notaPractica
                                    print("!Nota Registrada¡")
                    elif (opcionTrainer == 2):
                            print("Campers asignados:")
                            print(trainer['Grupos'])
                    elif (opcionTrainer == 3):
                       print("HORARIO Y MI SALÓN")
                       print(f"{trainer['Nombre']} Horario: {trainer['Jornada']}")
                    elif (opcionTrainer == 4):
                        print("Generar reporte y bajo rendimiento")
                            for camper in Campers:
                                if camper["Riesgo"] == "Alto":
                                    print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")
                            print("\n CAMPERS CON RENDIMIENTO BAJO (<60):")
                            for camper in Campers:
                                if camper["NotaPromedioGeneral"] < 60 and camper["NotaPromedioGeneral"] > 0:
                                    print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']}) - Promedio: {camper['NotaPromedioGeneral']}")
                    elif (opcionTrainer == 5):
                        print("Sesión cerrada. ¡Hasta pronto!")
                        booleanito3 = False
    elif (rolOpcion == 3): 
            idCoord = input("\n Ingresa tu número de identificación: ")
            for coord in Coordinador:
                if coord["#Identificacion"] == idCoord:
                    booleanito4 = True
                    while(booleanito4 == True):
                        print(f"\n Bienvenido Coordinador {coord['Nombre']}!")
                        print(f"MENÚ COORDINADOR - {coord['Nombre']}")
                        print("1. Registrar notas prueba inicial")
                        print("2. Asignar campers a rutas")
                        print("3. Crear nueva ruta de entrenamiento")
                        print("4. Generar reportes")
                        print("5. Ver campers en riesgo alto")
                        print("6. Cerrar sesión")
                        opcionCoord = int(input("Selecciona una opción: "))

                        if (opcionCoord == 1):
                            
                            print("\n REGISTRAR NOTAS PRUEBA INICIAL ")
                            idCamper = input("ID del camper:  ")
                            nota_teorica = float(input("Nota teórica (0-100):  "))
                            nota_practica = float(input("Nota práctica (0-100):  "))

                            for camper in Campers:

                                if camper["#Identificacion"] == idCamper:                                   
                                    camper["NotaPruebaTeorica"] = nota_teorica
                                    camper["NotaPruebaPractica"] = nota_practica
                                    promedio = (nota_teorica + nota_practica) / 2
                                    camper["NotaPromedioGeneral"] = promedio

                                    if promedio >= 60:
                                        camper["Estado"] = "Aprobado"
                                        print(f"Camper APROBADO con promedio: {promedio}")
                                    else:
                                        camper["Estado"] = "Reprobado"
                                        print(f"Camper REPROBADO con promedio: {promedio}")
                                    break
                            else:

                                print("Camper no encontrado.")
                        elif (opcionCoord == 2):
                            print("\n ASIGNAR CAMPERS A RUTAS ")
                            print("Rutas disponibles:")

                            for i, ruta in enumerate(rutas, 1):                               
                                print(f"{i}. {ruta['Nombre']}")
                            rutaSeleccion = int(input("Seleccione la ruta:  ")) - 1

                            if 0 <= rutaSeleccion < len(rutas):
                                rutaElegida = rutas[rutaSeleccion]["Nombre"]
                                print(f"\nGrupos disponibles para {rutaElegida}:")
                                for grupo in rutas[rutaSeleccion]["Grupos"]:

                                    print(f"- {grupo}")
                                grupoAsignar = input("Ingrese el grupo a asignar:  ")
                                idCamper = input("ID del camper:  ")
                                for camper in Campers:
                                    if camper["#Identificacion"] == idCamper:
                                        camper["Grupo"] = grupoAsignar
                                        for grupo in Grupos:
                                            if grupo["Nombre"] == grupoAsignar:
                                                camper["Trainer"] = grupo["Trainer"]
                                                break
                                        print(f"Camper asignado al grupo {grupoAsignar}")
                                        break
                                else:
                                    print("Camper no encontrado.")
                        elif (opcionCoord == 3):
                            print("\n CREAR NUEVA RUTA DE ENTRENAMIENTO ")
                            nombreRuta = input("Nombre de la nueva ruta:  ")
                            trainersRuta = input("Trainers (separados por coma):  ").split(",")
                            gruposRuta = input("Grupos (separados por coma):  ").split(",")
                            nuevaRuta = {
                                "Nombre": nombreRuta,
                                "Trainers": [t.strip() for t in trainersRuta],
                                "Grupos": [g.strip() for g in gruposRuta]
                            }
                            rutas.append(nuevaRuta)
                            print(f"Ruta '{nombreRuta}' creada exitosamente!")
                        elif (opcionCoord == 4):
                            print("\n GENERAR REPORTES ")
                            print("1. Reporte de campers aprobados")
                            print("2. Reporte de campers reprobados")
                            print("3. Reporte por grupo")
                            print("4. Reporte por trainer")
                            tipoReporte = int(input("Seleccione tipo de reporte:  "))                   
                            if tipoReporte == 1:
                                print("\n CAMPERS APROBADOS:")
                                for camper in Campers:
                                    if camper["Estado"] == "Aprobado":
                                        print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")                   
                            elif tipoReporte == 2:
                                print("\n CAMPERS REPROBADOS:")
                                for camper in Campers:
                                    if camper["Estado"] == "Reprobado":
                                        print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")                   
                            elif tipoReporte == 3:
                                grupoReporte = input("Ingrese el grupo:  ")
                                print(f"\n CAMPERS DEL GRUPO {grupoReporte}:")
                                for camper in Campers:
                                    if camper["Grupo"] == grupoReporte:
                                        print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")                   
                            elif tipoReporte == 4:
                                trainerReporte = input("Ingrese el trainer:  ")
                                print(f"\n CAMPERS DEL TRAINER {trainerReporte}:")
                                for camper in Campers:
                                    if camper["Trainer"] == trainerReporte:
                                        print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")
                        elif (opcionCoord == 5):
                            print("\n CAMPERS EN RIESGO ALTO: ")
                            for camper in Campers:
                                if camper["Riesgo"] == "Alto":
                                    print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']})")
                            print("\n CAMPERS CON PROMEDIO BAJO (<60):")
                            for camper in Campers:
                                if camper["NotaPromedioGeneral"] < 60 and camper["NotaPromedioGeneral"] > 0:
                                    print(f"   - {camper['Nombre']} {camper['Apellido']} (ID: {camper['#Identificacion']}) - Promedio: {camper['NotaPromedioGeneral']}")
                        elif (opcionCoord == 6):
                            print("\n Sesión cerrada. ¡Hasta pronto!")
                       
                            booleanito4 = False
    elif (rolOpcion == 4):
        print("¡Hasta luego!")
        booleanito = False  
