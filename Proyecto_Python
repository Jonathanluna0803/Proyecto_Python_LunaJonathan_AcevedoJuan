rutas = [{
    "Nombre":"NodeJS",
    "Trainers":["Pedro","Cristian"],
    "Grupos":["P2","Cr2","P3",]
},{
    "Nombre":"Java",
    "Trainers":["Pedro","Cristian","Jolver","Kevin","Carlos","Edwin"],
    "Grupos":["P1","Cr1","K1","Ca1","Ca2"]
},{
    "Nombre":"Netcore",
    "Trainers":["Jolver","Edwin"],
    "Grupos":["J1","E1"]
